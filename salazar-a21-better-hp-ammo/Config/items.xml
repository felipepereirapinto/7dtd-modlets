<root>
	<!-- 
	HP now has a 20% damage increase from regular ammo, instead of 10%, just like AP ammo.
	It's still cheaper to craft than AP ammo, but does not penetrate armor.
	- 9mmBulletHP:		35 => 38
	- 44MagnumBulletHP:	77 => 84
	- 762mmBulletHP:	52 => 57
	-->

	<set xpath="//effect_group[@name='ammo9mmBulletHP']/passive_effect[@name='EntityDamage' and @operation='base_set']/@value">38</set>
	<set xpath="//effect_group[@name='ammo44MagnumBulletHP']/passive_effect[@name='EntityDamage' and @operation='base_set']/@value">84</set>
	<set xpath="//effect_group[@name='ammo762mmBulletHP']/passive_effect[@name='EntityDamage' and @operation='base_set']/@value">57</set>

</root>